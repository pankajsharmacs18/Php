<form method="POST">
<label>Enter Range of Number </label>
<input type="text" name="range" size="10" >
<input type="submit" name="submit" >
</form>
<?php

	$range=$_POST['range'];
	if(isset($range)){
	$i='a';
	while($i<=$range)
	{
		echo "$i ";
		$i++;
	}
	}

?>
